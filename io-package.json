		"keywords": ["senec", "senec home", "battery"],
		"type":"api",
		"supportStopInstance":true,
		"objects": [
        {
            "_id": "_design/senec",
            "language": "javascript",
            "views": {
                "listDevices": {
                    "map": "function(doc) {\n  if (doc._id.match(/^senec\\.[0-9]+\\.\\*?[A-Za-z0-9_-]+(\\.[0-9]+)?$/)) {\n   emit(doc._id, {ADDRESS:(doc.native?doc.native.ADDRESS:''),VERSION:(doc.native?doc.native.VERSION:'')});\n  }\n}"
                },
                "paramsetDescription": {
                    "map": "function(doc) {\n  if (doc._id.match(/^senec\\.meta/) && doc.meta.type === 'paramsetDescription') {\n   emit(doc._id, doc);\n  }\n}"
                }
            }
        }
		],
		"instanceObjects": [
